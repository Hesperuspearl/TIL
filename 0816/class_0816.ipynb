   "source": [
    "def f(i, N):\n",
    "    if i == N:\n",
    "        print(A)\n",
    "    else:\n",
    "        for j in range(i, N):\n",
    "            A[i], A[j] = A[j], A[i]\n",
    "            f(i+1, N)\n",
    "            A[i], A[j] = A[j], A[i]\n",
    "\n",
    "A = [0, 1, 2]\n",
    "f(0, 3)"
   ]
